/* pages/home/personalTrainer.wxss */



.personal-cell {
    padding: 0 30rpx 15rpx;
    background-color: #22233A;
    margin-bottom: 15rpx;
}
.personal-headimg {
    width: 60rpx;
    height: 60rpx;
    border-radius: 50%;
    float: left;
    margin-right: 15rpx;
    margin-top: 16rpx;
}
.head-view {
    border-bottom: 2rpx #262439 solid;
    font-size: 32rpx;
    height: 100rpx;
    line-height: 100rpx;
}
.tel-icon {
    width: 42rpx;
    height: 42rpx;
    float: right;
    margin-top: 25rpx;
}

.class-view {
    color: #BAB9C7;
}
.class-view view {
    padding: 20rpx 0 0;
}

.remind-text {
    padding: 30rpx;
    color: #BAB9C7;
}
.bookingClass{
    float: right;
    color: #FF904C;
    border: 1rpx solid #FF904C;
    border-radius: 6rpx;
    width: 150rpx;
    height: 50rpx;
    text-align: center;
    line-height: 50rpx;
    margin-top: 25rpx;
}