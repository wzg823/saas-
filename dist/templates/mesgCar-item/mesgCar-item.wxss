
.mesgCar-cell {
    margin: 20rpx 30rpx;
    padding: 0 20rpx;
    border-radius: 10rpx;
    background-color: #22233A;
}
.mesgCar-cell .up-view {
    padding: 20rpx 0 20rpx;
    border-bottom: 1rpx #262439 solid;
}
.mesgCar-cell .up-view .left-icon {
    width: 39rpx;
    height: 29rpx;
    float: left;
    margin-top: 9rpx;
    margin-right: 20rpx;
}
.mesgCar-cell .up-view .time-text {
    float: right;
    font-size: 26rpx;
    color: #848296;
    margin-top: 5rpx;
}
.mesgCar-cell .down-view {
    padding: 20rpx 0 20rpx;
    color: #BAB9C7;
    font-size: 28rpx;
}