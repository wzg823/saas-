/* imgImgText-cell  */
.imgImgText-cell {
    margin-bottom:5rpx;
    padding:0 30rpx;
    background-color:#22233A;
    height:100rpx;
}
.imgImgText-cell .left-icon {
    width:90rpx;
    height:70rpx;
    float:left;
    margin-top:16rpx;
    position: absolute;
}
.imgImgText-cell .cellIcon {
    width: 40rpx;
    height:40rpx;
    float:left;
    position:absolute;
    margin-top:30rpx;
    margin-left:25rpx;
}
.imgImgText-cell text {
    margin-left:130rpx;
    display:inline-block;
    margin-top:30rpx;
}