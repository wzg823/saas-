
.textInput-view {
    height: 90rpx;
    line-height: 90rpx;
    background-color: #22233A;
    padding: 0 30rpx;
    margin-bottom: 5rpx;
}

.textInput-view .title-view {
    width: 180rpx;
    float: left;
}
.textInput-view input {
    margin-top: 20rpx;
    display: inline-block;
    width: 450rpx;
}

.textInput-view .checked-view {
    width: 29rpx;
    height: 29rpx;
    border: 1rpx white solid;
    border-radius: 50%;
    position: absolute;
    margin-top: 28rpx;
}
.textInput-view .checked-icon {
    width: 30rpx;
    height: 30rpx;
    position: absolute;
    margin-top: 28rpx;
}

.gender-view {
    display: inline-block;
    margin-right: 20rpx;
}
.gender-view text {
    margin-left: 50rpx;
}

.textInput-view .text-view {
    display: inline-block;
    color: #848296;
}
.textInput-view .textValue-view {
    display: inline-block;
    color: white;
}

.right-icon {
    margin-top: 30rpx;
    margin-right: 0;
}