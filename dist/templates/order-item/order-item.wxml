
<template name='order-item'>

  <view class='order-cell'>
    <view class='title-view'>{{ orderName }} <text>{{ orderTime }}</text> </view>
    <view class='info-view'>
      <view>订单号: <text class='whiteColor'>{{ orderId }}</text> </view>
      <view>
        <text> 价格: <text class='mainColor'>¥{{ price }}</text> </text>
        <text class='status-text'>{{ orderStatus }}</text>
      </view>
    </view>
  </view>

</template>