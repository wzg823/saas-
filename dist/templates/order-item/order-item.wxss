
.order-cell {
    padding: 0 30rpx 20rpx;
    background-color: #22233A;
    margin-top: 20rpx;
}
.order-cell .title-view {
    border-bottom: 1rpx #262439 solid;
    padding: 20rpx 0;
    font-size: 32rpx;
}
 .order-cell .title-view text {
    color: #BAB9C7;
    float: right;
    font-size: 26rpx;
    margin-top: 7rpx;
}

.order-cell .info-view {
    color: #BAB9C7;
}
.order-cell .info-view view{
    padding-top: 20rpx;
}

.order-cell .info-view .status-text {
    float: right;
    font-size: 26rpx;
    margin-top: 6rpx;
}